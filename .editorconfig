# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true


[/dist/**]
charset = none
end_of_line = none
indent_size = none
indent_style = none
insert_final_newline = none
trim_trailing_whitespace = none
